#include <bits/stdc++.h>
#include <iostream>

using namespace std;


int main(){
// int t; 
// cin >> t;
// for(int w = 0; w < t;w++){
long n;
cin >> n;
if(n == 1){cout << n;return 0;}
else if(n < 4){cout << "NO SOLUTION";return 0;}

deque<int> v;
for(int y = 0; y < n;y++){
    if(y == 1){continue;}
else if(y % 2){v.push_back(y+1);
}
else{v.push_front(y+1);}
}
v.push_back(2);
for(int y = 0; y < n;y++){
cout << v[y] << " ";}



//}

return 0;
}
